{
  "name": "My Extension",
  "version": "1.0",
  "manifest_version": 2,
  "description": "A simple extension with two buttons",
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "permissions": [
    "tabs"
  ],
  "browser_action": {
    "default_icon": "icon-48.png",
    "default_title": "My Extension",
    "default_popup": "popup.html"
  }
}
